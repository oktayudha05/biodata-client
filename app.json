{
  "expo": {
    "name": "Biodata Keluarga TIF Untidar",
    "slug": "biodata-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/himatif.png",
    "scheme": "biodataapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.oktayudha05.biodataapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6ec26698-7529-45f2-a82d-23a34934670a"
      }
    }
  }
}
